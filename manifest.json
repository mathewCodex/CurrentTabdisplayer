{
     "manifest_version" : 3,
    "name": "Current Tab Title",
 
    "version": "1.0",
      "description": "Current Tab extension",
      "permissions":[
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ], 
        "action":{
 "default_popup": "popup.html",
 "default_icon":{
        "16":"./image/pin.png",
        "48": "./image/pin.png",
        "128": "./image/pin.png"
    },
    "icons":{
        "16":"./image/pin.png",
        "48": "./image/pin.png",
        "128": "./image/pin.png"
    },
    "background": {
        "service_worker": "background.js"
    },
   
    "options_page":"options.html"

    }
    
}